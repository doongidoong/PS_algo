     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "for i in range(1,n+1):\n",
    "    if i%2 == 1:\n",
    "        print(i)\n",
    "\n",
    "sumn =0\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    sumn += i\n",
    "print(sumn)\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    if n % i == 0:\n",
    "        print(i)\n"
   ]
  }
